@model IEnumerable<JOB.WEB.Models.ServicoViewModel_api>

@{
    ViewBag.Title = "Suas Ofertas Criadas";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NOME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESC_ESPECIALIDADE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESC_SUB_ESPECIALIDADE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DT_CADASTRO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DS_TITULO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DS_OBSERVACOES)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VL_SUGERIDO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NOME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESC_ESPECIALIDADE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESC_SUB_ESPECIALIDADE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DT_CADASTRO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DS_TITULO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DS_OBSERVACOES)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VL_SUGERIDO)
        </td>
    </tr>
}

</table>

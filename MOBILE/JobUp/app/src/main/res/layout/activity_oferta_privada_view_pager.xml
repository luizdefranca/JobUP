<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context="com.br.jobup.fragments.OfertaPrivadaViewPagerActivity">
    <android.support.design.widget.TabLayout
        android:id="@+id/tblt_oferta_privada"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimaryLight"
        app:tabBackground="@color/colorPrimary"
        app:tabSelectedTextColor="@color/secondaryText"
        app:tabTextColor="@color/primaryText"/>

    <android.support.v4.view.ViewPager
        android:layout_alignBottom="@+id/tblt_oferta_privada"
        android:id="@+id/viewPageOfertaPrivada"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>
